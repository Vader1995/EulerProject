    <application
            android:name="net.jarydrester.euler.EulerApplication"
            android:allowBackup="true"
            android:icon="@drawable/ic_launcher"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
        <activity android:name="net.jarydrester.euler.activities.ProblemListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="net.jarydrester.euler.activities.ProblemActivity"/>
    </application>

</manifest>
